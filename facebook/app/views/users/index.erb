<html>
<body>
<center>
<h3>All user details</h3></center>
<br>
<table align="center" border=1>
<th>Name</th><th>Gender</th><th>Email</th><th>Date of birth</th>
<% @users.each do |user| %>
<tr>
<td>
	<%= user.name %>
</td>
<td>
	<%= user.gender %>
</td>
<td>
	<%= user.email %>
</td>
<td>
	<%= user.date_of_birth %>
</td>

<tr>
<% end %>

</table>
</body>
</html>
